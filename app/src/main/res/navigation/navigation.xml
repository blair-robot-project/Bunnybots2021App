<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation.xml"
  app:startDestination="@id/mainContainerFragment">

  <fragment
    android:id="@+id/mainContainerFragment"
    android:name="team449.frc.refereeappbase.fragment.MatchContainerFragment"
    android:label="MainContainerFragment" >
    <action
      android:id="@+id/action_mainContainerFragment_to_settingsFragment"
      app:destination="@id/settingsFragment"/>
  </fragment>

  <fragment
    android:id="@+id/settingsFragment"
    android:name="team449.frc.refereeappbase.fragment.SettingsFragment"
    android:label="SettingsFragment" >
    <action
      android:id="@+id/action_settingsFragment_to_mainContainerFragment"
      app:destination="@id/mainContainerFragment" />
  </fragment>

</navigation>